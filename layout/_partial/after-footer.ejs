
<%- js('//cdn.jsdelivr.net/npm/jquery@2.2.4/dist/jquery.min.js') %>
<%- js('//cdn.jsdelivr.net/npm/bootstrap@3.4.1/dist/js/bootstrap.min.js') %>

<% if (is_post()) { %>
    <%- js('//cdn.jsdelivr.net/npm/@fancyapps/fancybox@3.5.7/dist/jquery.fancybox.min.js') %>
<% } %>

<%if (is_current('tags/index.html')){ %>
    <%- js('/js/jquery.tagcanvas.min.js') %>
<% } %>

<%- js('/js/script.min.js') %>

<!-- Disqus Comments -->
<% if (theme.comments && theme.comments.disqus_shortname){ %>
<script type="text/javascript">
    var disqus_shortname = '<%= theme.comments.disqus_shortname %>';

    (function(){
        var dsq = document.createElement('script');
        dsq.type = 'text/javascript';
        dsq.async = true;
        dsq.src = '//' + disqus_shortname + '.disqus.com/<% if (page.comments){ %>embed.js<% } else { %>count.js<% } %>';
        (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(dsq);
    }());
</script>
<% } %>




