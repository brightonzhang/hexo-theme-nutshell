<!-- Global site tag (gtag.js) - Google Analytics -->
<% if (site.data.analytics.google ) { %>
    <script async src="https://www.googletagmanager.com/gtag/js?id=<%- site.data.analytics.google.tracking_id %>"></script>
    <script>
    window.dataLayer = window.dataLayer || [];
    function gtag(){dataLayer.push(arguments);}
    gtag('js', new Date());

    gtag('config', '<%- site.data.analytics.google.tracking_id %>');
    </script>
<% } %>
