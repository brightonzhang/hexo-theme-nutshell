<% switch (page.type) {
case 'tag' :
case 'tags' : %>
    <%- partial('_partial/header', {cover: site.data.cover.tag, title: page.title}) %>
    <%- partial('_partial/tag-index') %>
    <% break;

case 'category' :
case 'categories' : %>
    <%- partial('_partial/header', {cover: site.data.cover.category, title: page.title}) %>
    <%- partial('_partial/category-index') %>
    <% break;

default : %>
    <%- partial('_partial/header', {cover: page.cover, title: page.title}) %>
    <%- partial('_partial/post-article', {post: page}) %>
    <% break;
} %>


