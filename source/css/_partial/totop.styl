#totop
    background-color: #eee;
    border-radius: 100%;
    height: 48px;
    width: 48px;
    position: fixed;
    right:  3%;
    bottom: -48px;
    z-index: 99;
    -webkit-transition: 0.5s;
    -moz-transition: 0.5s;
    transition: 0.5s;
    &.display
        bottom: 5%;

#totop-percent 
    @extend .sans-serif
    font-size: 24px;
    height: 48px;
    line-height: 48px;
    position: absolute;
    text-align: center;
    top: 0;
    width: 48px;
    color: #555;
    cursor: pointer;
    &:before
        content:attr(data-percent);
    &:hover:before 
        display: inline-block;
        font-family: "FontAwesome"
        text-rendering: auto;
        -webkit-font-smoothing: antialiased;
        -moz-osx-font-smoothing: grayscale;
        content: "\f062";

